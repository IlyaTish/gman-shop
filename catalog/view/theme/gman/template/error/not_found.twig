{{ header }}
<div id="error-not-found" class="error">
  <div class="breadcrumb__nav">
    <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
        <li>
          <a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>

  <div class="error__content">
    <h1>{{ heading_title }}</h1>
    <p>{{ text_error }}</p>
    <div class="error__continue">
      <a href="{{ continue }}" class="btn-primary">{{ button_continue }}</a>
    </div>
  </div>
</div>
{{ footer }}
